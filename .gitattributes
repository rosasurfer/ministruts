*.html       text eol=lf
*.ini        text eol=lf
*.js         text eol=lf
*.json       text eol=lf
*.lock       text eol=lf
*.md         text eol=lf
*.php        text eol=lf
*.properties text eol=lf
*.sh         text eol=lf
*.txt        text eol=lf
*.xml        text eol=lf
*.yml        text eol=lf
.git*        text eol=lf


