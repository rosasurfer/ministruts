{
    "name":        "my-namespace/ministruts-demo",
    "description": "Demo application using the MiniStruts framework",
    "type":        "project",
    "license":     "proprietary",
    "config": {
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/rosasurfer/ministruts"
        }
    ],
    "require": {
        "php":                   ">=5.6",
        "rosasurfer/ministruts": "dev-master"
    },
    "minimum-stability": "stable",
    "autoload": {
        "classmap": ["app/"]
    }
}
