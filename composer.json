{
    "name":        "rosasurfer/ministruts",
    "description": "Java Struts inspired MVC micro framework for PHP",
    "type":        "library",
    "license":     "WTFPL",
    "authors": [{
        "name":  "Peter Walther",
        "email": "ministruts@rosasurfer.com"
    }],
    "config": {
        "platform": {
            "php": "5.6.40"
        },
        "optimize-autoloader": true
    },
    "require": {
        "php":           "^5.6 || ^7.0",
        "ext-ctype":     "*",
        "ext-filter":    "*",
        "ext-json":      "*",
        "ext-libxml":    "*",
        "ext-mbstring":  "*",
        "ext-simplexml": "*",
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "rosasurfer\\": "src"
        },
        "files": ["src/load.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10.x-dev"
        }
    }
}
