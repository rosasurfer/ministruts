{
    "name":        "rosasurfer/ministruts",
    "description": "Java Struts inspired MVC micro framework for PHP",
    "type":        "library",
    "license":     "WTFPL",
    "authors": [{
        "name":  "Peter Walther",
        "email": "ministruts@rosasurfer.com"
    }],
    "config": {
        "platform": {
            "php": "5.6.40"
        },
        "optimize-autoloader": true
    },
    "require": {
        "php":          "^5.6",
        "ext-ctype":    "*",
        "ext-json":     "*",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "rosasurfer\\": "src"
        },
        "files": ["src/load.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-0.8": "0.8.x-dev"
        }
    }
}
